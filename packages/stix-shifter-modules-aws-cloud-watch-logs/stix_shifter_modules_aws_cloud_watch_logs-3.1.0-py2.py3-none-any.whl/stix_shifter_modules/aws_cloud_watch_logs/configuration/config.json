{
    "connection": {
        "type": {
            "displayName": "Amazon CloudWatch Logs",
            "type": "connectorType"
        },
        "help": {
            "type": "link",
            "default": "https://www.ibm.com/support/knowledgecenter/SSTDPP_1.1.0/docs/scp-core/data-sources-aws.html"
        },
        "region": {
            "type": "text",
            "previous": "connection.options.region"
        },
        "log_group_names": {
            "type": "text",
            "optional": true,
            "previous": "connection.options.log_group_names"
        },
        "options": {
            "type": "fields",
            "result_limit": {
                "default": 10000,
                "min": 1,
                "type": "number",
                "previous": "connection.resultSizeLimit"
            },
            "time_range": {
                "default": 5,
                "min": 1,
                "type": "number",
                "previous": "connection.timerange",
                "nullable": true
            },
            "timeout": {
                "default": 30,
                "min": 1,
                "type": "number",
                "previous": "connection.timeoutLimit"
            },
            "dialects": {
                "type": "array",
                "default": null,
                "hidden": true,
                "previous": "connection.dialects"
            },
            "validate_pattern": {
                "type": "boolean",
                "optional": true,
                "hidden": true,
                "previous": "connection.validate_pattern"
            },
            "stix_validator": {
                "type": "boolean",
                "optional": true,
                "hidden": true,
                "previous": "connection.stix_validator"
            },
            "mapping": {
                "type": "json",
                "optional": true,
                "hidden": true,
                "previous": "connection.mapping"
            }
        }
    },
    "configuration": {
        "auth": {
            "type": "fields",
            "aws_access_key_id": {
                "type": "password"
            },
            "aws_iam_role": {
                "type": "password",
                "optional": true
            },
            "aws_secret_access_key": {
                "type": "password"
            }
        }
    }
}