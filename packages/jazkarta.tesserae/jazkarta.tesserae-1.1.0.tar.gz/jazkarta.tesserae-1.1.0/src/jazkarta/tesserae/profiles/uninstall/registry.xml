<?xml version="1.0"?>
<registry>

  <record name="plone.app.mosaic.default_available_actions">
    <value purge="False">
      <element remove="True">tile-full-width</element>
      <element remove="True">tile-light-text</element>
    </value>
  </record>

  <records interface="plone.app.mosaic.interfaces.IFormat"
           prefix="plone.app.mosaic.formats.tile_full_width" remove="True">
  </records>

  <records interface="plone.app.mosaic.interfaces.IFormat"
           prefix="plone.app.mosaic.formats.tile_light_text" remove="True">
  </records>

  <records prefix="plone.resources/jazkarta-tesserae-mosaic"
           interface='Products.CMFPlone.interfaces.IResourceRegistry'
           remove="True">
  </records>

  <records prefix="plone.resources/bootstrap-carousel"
           interface='Products.CMFPlone.interfaces.IResourceRegistry'
           remove="True">
  </records>

  <records prefix="plone.resources/jazkarta-tesserae"
           interface='Products.CMFPlone.interfaces.IResourceRegistry'
           remove="True">
  </records>

  <records prefix="plone.bundles/jazkarta-tesserae"
           interface='Products.CMFPlone.interfaces.IBundleRegistry'
           remove="True">
  </records>

  <record name="plone.app.tiles">
    <value purge="false">
      <element remove="True">jazkarta.tesserae.content_summary</element>
      <element remove="True">jazkarta.tesserae.collection_summary</element>
      <element remove="True">jazkarta.tesserae.background_image</element>
    </value>
  </record>

  <records prefix="plone.app.mosaic.app_tiles.jazkarta_tesserae_content_summary"
           interface="plone.app.mosaic.interfaces.ITile" remove="True" />
  <records prefix="plone.app.mosaic.app_tiles.jazkarta_tesserae_collection_summary"
           interface="plone.app.mosaic.interfaces.ITile" remove="True" />
  <records prefix="plone.app.mosaic.app_tiles.jazkarta_tesserae_background_image"
           interface="plone.app.mosaic.interfaces.ITile" remove="True" />

</registry>
